{
	"$bankSchema": {
		"$schema": "http://json-schema.org/draft-04/schema#",
		"title": "TranslatorJson",
		"description": "A loan object",
		"type": "object",
		"properties": {
			"type": "object",
			"maxProperties": 4,
			"ssn": {
				"description": "social security number. Consist of 8 numbers",
				"type": "string"
			},
			"creditScore": {
				"description": "credit score calculated by a calcualtor",
				"type": "string"
			},
			"loanAmount": {
				"description": "requested loan amount",
				"type": "integer"
			},
			"loanDuration": {
				"description": "requested loan duration",
				"type": "date"
			},
			"required": ["ssn", "creditScore", "loanAmount", "loanDuration"]
		}
	}
}